<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>io.nats.natshd</string>
    <key>ProgramArguments</key>
    <array>
        <string>SYSTEM_BIN_DIR_PLACEHOLDER/natshd</string>
        <string>-config</string>
        <string>SYSTEM_CONFIG_DIR_PLACEHOLDER/config.toml</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/var/log/natshd.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/natshd.error.log</string>
    <key>UserName</key>
    <string>_natshd</string>
    <key>GroupName</key>
    <string>_natshd</string>
</dict>
</plist>
